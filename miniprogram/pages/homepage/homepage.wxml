<view class="cu-bar bg-black search ">
  <view class="cu-avatar round" style="background-image:url(https://image.weilanwl.com/img/square-1.jpg);"></view>
  <view class="search-form radius">
    <text class="icon-search"></text>
    <input type="text" placeholder="搜索图片、文章、视频" confirm-type="search"></input>
  </view>
  <view class="action">
    <text>广西</text>
    <text class="icon-triangledownfill"></text>
  </view>
</view>

<scroll-view style="width:100%;height: 100%;" scroll-y="true" bindscroll="onPageScroll">
  <view class="{{top>210 ? 'topnav' : ''}}">
    <swiper class="card-swiper square-dot" indicator-dots="true" circular="true" autoplay="true" interval="5000" duration="500" bindchange="cardSwiper" indicator-color="#8799a3" indicator-active-color="#0081ff">
      <swiper-item wx:for="{{swiperList}}" wx:key="index" class="{{cardCur==index?'cur':''}}">
        <view class="swiper-item">
          <image src="{{item.url}}" mode="aspectFill" wx:if="{{item.type=='image'}}"></image>
          <video src="{{item.url}}" autoplay loop muted show-play-btn="{{false}}" controls="{{false}}" objectFit="cover" wx:if="{{item.type=='video'}}"></video>
        </view>
      </swiper-item>
    </swiper>

    <scroll-view scroll-x class="bg-blue nav text-center shadow-blur light ">
      <view class="cu-item {{index==TabCur?'text-black cur':''}} text-bold" wx:for="{{3}}" wx:key bindtap="tabSelect" data-id="{{index}}">
        {{ navList[index].title }}
      </view>
    </scroll-view>



    <swiper current="{{TabCur}}" class="swiper" style="height:94.5Vh;" bindchange="swiperTab" duration="300" circular="true">
      <swiper-item style="height:100%">
        <scroll-view scroll-y style="height:100%">
          <view class="news_box">
            <block wx:for="{{ newsdata }}" wx:key="index">
              <!-- <view data-alpha-beta="{{  item.id  }}" bindtap="bindViewTap"> DataSet Test </view> -->
              <view class="cu-card article isCard" data-worker-Id='{{  item.id  }}' bindtap="get_item_id">
                <view class="cu-item shadow">
                  <view class="title text-bold bg-grey shadow-blur light">
                    <view class="text-cut text-lg text-bold  ">{{ item.title }}</view>
                  </view>
                  <view class="content ">
                    <!-- <image src="https://ossweb-img.qq.com/images/lol/web201310/skin/big10006.jpg" mode="aspectFill"></image> -->
                    <view class="desc">
                      <view class="text-content">{{ item.article[0] }}</view>
                      <view>
                        <view class="cu-tag bg-red light sm round text-cut" style="width:90px">{{ item.detail[0] }}</view>
                        <view class="cu-tag bg-green light sm round text-cut" style="width:90px">{{ item.detail[1] }}</view>
                      </view>
                    </view>
                  </view>
                </view>
              </view>
            </block>
          </view>
        </scroll-view>
      </swiper-item>
      <swiper-item style="height:100%">
        <scroll-view scroll-y style="height:100%">
          <view class="news_box">
            <block wx:for="{{ newsdata }}" wx:key="index">
              <!-- <view data-alpha-beta="{{  item.id  }}" bindtap="bindViewTap"> DataSet Test </view> -->
              <view class="cu-card article isCard" data-worker-Id='{{  item.id  }}' bindtap="get_item_id">
                <view class="cu-item shadow">
                  <view class="title text-bold bg-grey shadow-blur light">
                    <view class="text-cut text-lg text-bold  ">{{ item.title }}</view>
                  </view>
                  <view class="content ">
                    <!-- <image src="https://ossweb-img.qq.com/images/lol/web201310/skin/big10006.jpg" mode="aspectFill"></image> -->
                    <view class="desc">
                      <view class="text-content">{{ item.article[0] }}</view>
                      <view>
                        <view class="cu-tag bg-red light sm round text-cut" style="width:90px">{{ item.detail[0] }}</view>
                        <view class="cu-tag bg-green light sm round text-cut" style="width:90px">{{ item.detail[1] }}</view>
                      </view>
                    </view>
                  </view>
                </view>
              </view>
            </block>
          </view>
        </scroll-view>
      </swiper-item>
      <swiper-item style="height:100%">
        <scroll-view scroll-y style="height:100%">
          <view class="news_box">
            <block wx:for="{{ newsdata }}" wx:key="index">
              <!-- <view data-alpha-beta="{{  item.id  }}" bindtap="bindViewTap"> DataSet Test </view> -->
              <view class="cu-card article isCard" data-worker-Id='{{  item.id  }}' bindtap="get_item_id">
                <view class="cu-item shadow">
                  <view class="title text-bold bg-grey shadow-blur light">
                    <view class="text-cut text-lg text-bold  ">{{ item.title }}</view>
                  </view>
                  <view class="content ">
                    <!-- <image src="https://ossweb-img.qq.com/images/lol/web201310/skin/big10006.jpg" mode="aspectFill"></image> -->
                    <view class="desc">
                      <view class="text-content">{{ item.article[0] }}</view>
                      <view>
                        <view class="cu-tag bg-red light sm round text-cut" style="width:90px">{{ item.detail[0] }}</view>
                        <view class="cu-tag bg-green light sm round text-cut" style="width:90px">{{ item.detail[1] }}</view>
                      </view>
                    </view>
                  </view>
                </view>
              </view>
            </block>
          </view>
        </scroll-view>
      </swiper-item>
    </swiper>

  </view>
</scroll-view>




