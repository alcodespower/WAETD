/* pages/details/details.wxss */

.content_url_img {
  width: 100%;
  height: 60Vh;
  overflow: hidden;
}

.phone_box {
  width: 90%;
  height: 6Vh;
  margin: 0 5% 30rpx 5%;
  /* background-color: red; */
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid rgba(128, 128, 128, 0.411);
  border-radius: 5px;
}
.user_avatar_box{
  height: 200rpx;
  width: 200rpx;
  position: fixed;
  bottom: 36Vh;
  right: 20rpx;
  z-index: 99;
  
}
.avatar_set{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  border: 2px solid rgba(0, 0, 0, 0.404);
}
.phone_num {
  width: 70%;
  /* background-color: green; */
  display: flex;
  justify-content: center;
  align-items: center;
}

.model_zfjw{
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: 999;
  background-color: rgba(0, 0, 0, 0.384)
}
.bg_img_box{
  margin-top: 20rpx;
  width: 100%;
  height: 40%;
  background-image: url('https://6574-etd-lxra1-1301840515.tcb.qcloud.la/ETD/images/bg_login.jpg?sign=bbf08825182d6c84b5f055a7db13456c&t=1588704932');
  background-size: 70% 100%;
  background-repeat: no-repeat;
  background-position: center;
}
.control_box{
  margin-top: 20rpx;
  width: 100%;
  height: 50%;
  display: flex;
  justify-content: center; 
  align-items: center;
}
.login_box{
  margin-top: 15Vh;
  margin-left: 10%;
  width: 80%;
  height: 70Vh;
  background-color: white;
  border-top:none;
  border-radius: 5px;
}
